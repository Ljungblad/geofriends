{
  "expo": {
    "name": "geofriends",
    "slug": "geofriends",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/pin_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3fa182"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location."
      }
    },
    "android": {
      "package": "com.geofriends.geofriends",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
